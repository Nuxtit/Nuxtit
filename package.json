{
  "name": "redusa",
  "version": "1.0.0",
  "description": "Redusa - Nuxt Reddit Client",
  "author": "Redusa",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && netlify-lambda build ./functions/",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lambda": "netlify-lambda serve .",
    "lighthouse": "CHROME_PATH=/usr/bin/chromium-browser npx lighthouse-badges --urls https://redusa.netlify.com -o badges",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@nuxtjs/axios": "^5.8.0",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/proxy": "^1.3.1",
    "@nuxtjs/sentry": "^3.0.0",
    "async-mutex": "^0.1.3",
    "axios-retry": "^3.1.2",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.13",
    "cross-env": "^5.2.0",
    "date-fns": "1",
    "dotenv": "^8.1.0",
    "eslint": "5.16.0",
    "fontello-cli": "^0.4.0",
    "idb-keyval": "^3.2.0",
    "mark.js": "^8.11.1",
    "netlify-lambda": "^1.4.5",
    "node-sass": "^4.10.0",
    "nuxt": "^2.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "qs": "^6.5.2",
    "sass-loader": "^7.1.0",
    "shared-worker-loader": "^0.2.0",
    "shell_exec": "^1.0.3",
    "tumblr.js": "^2.0.0",
    "vue-backtotop": "^1.4.12"
  },
  "devDependencies": {
    "argv": "^0.0.2",
    "babel-eslint": "^10.0.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "lighthouse-badges": "^1.0.20",
    "nodemon": "^1.11.0",
    "prettier": "1.18.2"
  }
}
